@{
    Layout = "~/Views/Shared/_OuterLayout.cshtml";
    ViewBag.Title = "";
}
<script type="text/javascript">
    $(function () {
        function redirect() {
            var strUrl = "http://" + window.location.host + "/Login";
        }

        setInterval(function () { redirect() }, 5000);
    });

    function GoBackToLogin() {
        window.location.href = "/Login";
    }
</script>
<div class="col-md-12">
    <div id="inner-right-area">
        <div class="container">

            <div class="col-md-12">
                <div class="shadow-container" style="padding:36px 0 0 0;">
                    <div class="alert alert-success">Password has been changed successfully.</div>

                </div>
            </div>
            <div class="col-md-3"></div>
            <div class="col-md-6">
                <div class="search-panel">

                    <div class="col-md-12 text-center div-space-one">
                        <button id="submit" name="submit" type="submit" value="Go To Login" class="submit-btn" tabindex="1" onclick="GoBackToLogin()">Go To Login</button>

                    </div>
                </div>
            </div>
            <div class="col-md-3"></div>
        </div>
    </div>
</div>